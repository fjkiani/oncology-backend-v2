# Oncology Backend Environment Configuration
# Copy this to .env and fill in your actual values

# API Keys
GOOGLE_API_KEY=your_google_api_key_here
OPENAI_API_KEY=your_openai_api_key_here

# Database Configuration
DATABASE_URL=sqlite:///./oncology.db
CHROMA_DB_PATH=./chroma_db

# External Service URLs
FUSION_ENGINE_URL=https://crispro--fusion-engine-v1-fusionengine-api.modal.run
EVO_SERVICE_URL=your_evo_service_url_here
ZETA_ORACLE_URL=your_zeta_oracle_url_here

# CORS Origins (comma-separated)
ALLOWED_ORIGINS=http://localhost:3000,http://localhost:5173,https://your-frontend-domain.vercel.app

# WebSocket Configuration
WEBSOCKET_ENABLED=true
MAX_CONNECTIONS=100

# Logging
LOG_LEVEL=INFO
LOG_FILE=oncology_backend.log

# Security
SECRET_KEY=your_secret_key_here
TOKEN_EXPIRE_MINUTES=60

# Feature Flags
ENABLE_AGENT_SYSTEM=true
ENABLE_WEBSOCKET=true
ENABLE_DATABASE=true 